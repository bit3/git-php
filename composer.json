{
  "name": "bit3/git-php",
  "description": "Easy to use GIT wrapper for PHP.",
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "psr/log": "^1.0 || ^2.0 || ^3.0",
    "symfony/process": "^4.0 || ^5.0 || ^6.0"
  },
  "require-dev": {
    "phpcq/runner-bootstrap": "^1.0@dev",
    "symfony/filesystem": "^4.0 || ^5.0 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "Bit3\\GitPhp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bit3\\GitPhp\\Test\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-release/1.6.0": "1.6.0-dev"
    }
  }
}
